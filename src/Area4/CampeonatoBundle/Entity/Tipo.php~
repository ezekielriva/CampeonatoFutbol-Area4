<?php

namespace Area4\CampeonatoBundle\Entity;

use Doctrine\ORM\Mapping as ORM;
use Symfony\Component\Validator\Constraints as Assert;

/**
 * Area4\CampeonatoBundle\Entity\Tipo
 *
 * @ORM\Table()
 * @ORM\Entity(repositoryClass="Area4\CampeonatoBundle\Entity\TipoRepository")
 */
class Tipo
{
    /**
     * @var integer $id
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var string $nombre
     *
     * @ORM\Column(name="nombre", type="string", length=30)
     */
    private $nombre;

    /**
     * @var string $lista
     *
     * @ORM\Column(name="lista", type="string", length=255)
     */
    private $lista;

    /**
     * @var string $col1
     *
     * @ORM\Column(name="col1", type="string", length=255, nullable=true)
     */
    private $col1;

    /**
     * @var string $col2
     * 
     * @ORM\Column(name="col2", type="string", length=255, nullable=true)
     */
    private $col2;

    /**
     * @var string $col3
     *
     * @ORM\Column(name="col3", type="string", length=255, nullable=true)
     */
    private $col3;

    /**
     * @var string $col4
     *
     * @ORM\Column(name="col4", type="string", length=255, nullable=true)
     */
    private $col4;

    /**
     * @var string $col5
     *
     * @ORM\Column(name="col5", type="string", length=255, nullable=true)
     */
    private $col5;

    /**
     * @var string $col6
     *
     * @ORM\Column(name="col6", type="string", length=255, nullable=true)
     */
    private $col6;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set nombre
     *
     * @param string $nombre
     */
    public function setNombre($nombre)
    {
        $this->nombre = $nombre;
    }

    /**
     * Get nombre
     *
     * @return string 
     */
    public function getNombre()
    {
        return $this->nombre;
    }

    /**
     * Set lista
     *
     * @param string $lista
     */
    public function setLista($lista)
    {
        $this->lista = $lista;
    }

    /**
     * Get lista
     *
     * @return string 
     */
    public function getLista()
    {
        return $this->lista;
    }

    /**
     * Set col1
     *
     * @param string $col1
     */
    public function setCol1($col1)
    {
        $this->col1 = $col1;
    }

    /**
     * Get col1
     *
     * @return string 
     */
    public function getCol1()
    {
        return $this->col1;
    }

    /**
     * Set col2
     *
     * @param string $col2
     */
    public function setCol2($col2)
    {
        $this->col2 = $col2;
    }

    /**
     * Get col2
     *
     * @return string 
     */
    public function getCol2()
    {
        return $this->col2;
    }

    /**
     * Set col3
     *
     * @param string $col3
     */
    public function setCol3($col3)
    {
        $this->col3 = $col3;
    }

    /**
     * Get col3
     *
     * @return string 
     */
    public function getCol3()
    {
        return $this->col3;
    }

    /**
     * Set col4
     *
     * @param string $col4
     */
    public function setCol4($col4)
    {
        $this->col4 = $col4;
    }

    /**
     * Get col4
     *
     * @return string 
     */
    public function getCol4()
    {
        return $this->col4;
    }

    /**
     * Set col5
     *
     * @param string $col5
     */
    public function setCol5($col5)
    {
        $this->col5 = $col5;
    }

    /**
     * Get col5
     *
     * @return string 
     */
    public function getCol5()
    {
        return $this->col5;
    }

    /**
     * Set col6
     *
     * @param string $col6
     */
    public function setCol6($col6)
    {
        $this->col6 = $col6;
    }

    /**
     * Get col6
     *
     * @return string 
     */
    public function getCol6()
    {
        return $this->col6;
    }
		
		public function __toString() {
							return $this->getNombre();
}
}