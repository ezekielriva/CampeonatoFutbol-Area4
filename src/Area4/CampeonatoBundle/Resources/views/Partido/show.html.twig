<h1>Partido</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Dia</th>
            <td>{{ entity.dia|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Hora</th>
            <td>{{ entity.hora }}</td>
        </tr>
        <tr>
            <th>Fecha</th>
            <td>{{ entity.fecha }}</td>
        </tr>
        <tr>
            <th>Resultadol</th>
            <td>{{ entity.resultadol }}</td>
        </tr>
        <tr>
            <th>Resultadov</th>
            <td>{{ entity.resultadov }}</td>
        </tr>
        <tr>
            <th>Estado</th>
            <td>{{ entity.estado }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('partido') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('partido_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('partido_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
