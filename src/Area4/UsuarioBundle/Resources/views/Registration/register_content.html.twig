{% form_theme form '::form_register.html.twig' %}
{% form_theme formJugador '::form_register.html.twig' %}

<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register" id="formUsuarioNew">
	<fieldset>
		<legend>Datos del Usuario</legend>
    	{{ form_widget(form) }}
    </fieldset>
    <br/>
    <div>
    <button value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" 
    	onclick="submitForm()"/>
	</div>
</form>


