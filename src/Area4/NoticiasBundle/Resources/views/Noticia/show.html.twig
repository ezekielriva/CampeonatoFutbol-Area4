<h1>Noticia</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>                <td>{{ entity.id }}</td>        </tr>
        <tr>
            <th>Titulo</th>                <td>{{ entity.titulo }}</td>        </tr>
        <tr>
            <th>Texto</th>                <td>{{ entity.texto }}</td>        </tr>
        <tr>
            <th>Created_at</th>                <td>{{ entity.createdat|date('Y-m-d H:i:s') }}</td>        </tr>
        <tr>
            <th>Updated_at</th>                <td>{{ entity.updatedat|date('Y-m-d H:i:s') }}</td>        </tr>
        <tr>
            <th>Foto</th>                <td>{{ entity.foto }}</td>        </tr>
        <tr>
            <th>Activa</th>                <td>{{ entity.activa }}</td>        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('noticia') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('noticia_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('noticia_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
