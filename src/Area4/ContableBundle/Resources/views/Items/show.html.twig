<h1>Items</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Cantidad</th>
            <td>{{ entity.cantidad }}</td>
        </tr>
        <tr>
            <th>Precio_u</th>
            <td>{{ entity.preciou }}</td>
        </tr>
        <tr>
            <th>Precio_t</th>
            <td>{{ entity.preciot }}</td>
        </tr>
        <tr>
            <th>Created_at</th>
            <td>{{ entity.createdat|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Updated_at</th>
            <td>{{ entity.updatedat|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('items') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('items_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('items_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
